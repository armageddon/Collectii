
<h1>Log In</h1> <br>

<form>
<table>      <tr><td> 
    <label for="name">Name:</label>
</td> <td>
    <%= text_field_tag :name, params[:name] %>
</td> </tr>

  <tr><td> 
    <label for="password">Password:</label>
</td> <td>
    <%= password_field_tag :password, params[:password] %>
</td> </tr>
</table>
<br><br>
    <%= submit_tag "Login" %>

</form>
